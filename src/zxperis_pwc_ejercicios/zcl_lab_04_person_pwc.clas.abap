CLASS zcl_lab_04_person_pwc DEFINITION
  PUBLIC
  FINAL
  CREATE PUBLIC .

  PUBLIC SECTION.
  METHODS SET_AGE
  IMPORTING IV_AGE TYPE I.

  METHODS GET_AGE
